---
import Command from '../components/command.astro';
import Layout from '../layouts/base.astro';

const pageName = Astro.url.pathname.slice(1).replace(/^\/?(.+?)\/?$/, '$1');
---

<Layout title="Page Not Found - rileyflynn.me" description="The requested page could not be found.">
    <Command command={`kubectl describe page ${pageName}`} />
    Error from server (NotFound): page "{pageName}" not found
</Layout>
